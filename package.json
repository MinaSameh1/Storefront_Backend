{
  "name": "project2",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "build:watch": "tsc -w",
    "dev": "cross-env NODE_ENV='development' ts-node-dev --respawn ./src/index.ts",
    "debug": "ts-node-dev --transpile-only --respawn --inspect=9229 src/index.ts",
    "lint": "eslint src/*.ts",
    "migrate:dev:up": "cross-env NODE_ENV='development' db-migrate up --dotenvCustomPath .env.development",
    "migrate:dev:down": "cross-env NODE_ENV='development' db-migrate down --dotenvCustomPath .env.development",
    "migrate:dev:reset": "cross-env NODE_ENV='development' db-migrate reset --dotenvCustomPath .env.development",
    "migrate:prod:up": "cross-env NODE_ENV='production' db-migrate --dotenvCustomPath .env.production up",
    "migrate:prod:down": "cross-env NODE_ENV='production' db-migrate --dotenvCustomPath .env.production down",
    "migrate:prod:reset": "cross-env NODE_ENV='production' db-migrate --dotenvCustomPath .env.production reset",
    "fix": "eslint --fix src/*.ts",
    "format": "prettier --write 'src/**/*.ts'",
    "db:up": "docker-compose up -d && npm run migrate:up",
    "migrate:test:up": "cross-env NODE_ENV='test' db-migrate --dotenvCustomPath .env.test up -c 4",
    "migrate:test:down": "cross-env NODE_ENV='test' db-migrate --dotenvCustomPath .env.test down -c 4",
    "migrate:test:reset": "cross-env NODE_ENV='test' db-migrate --dotenvCustomPath .env.test reset -c 4",
    "test": "npm run migrate:test:reset && npm run migrate:test:up && npm run build && cross-env NODE_ENV='test' jasmine",
    "prepare": "husky install",
    "pre-commit": "lint-staged && npm run test"
  },
  "keywords": [
    "typescript",
    "postgres",
    "udacity",
    "express"
  ],
  "author": "Mina Sameh Wadie",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-pino-logger": "^4.0.3",
    "@types/jasmine": "^4.3.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.22",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "cross-env": "^7.0.3",
    "db-migrate": "^1.0.0-beta.18",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^7.0.0",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-pino-logger": "^7.0.0",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "pino": "^8.6.0",
    "pino-pretty": "^9.1.0"
  }
}
